{
  "Comment": "Ensure folder.",
  "version": "1.0",
  "StartAt": "EnsureFolder",
  "States": {
    "EnsureFolder": {
      "Type": "Task",
      "Resource": "module:ensureCloudStorageFolder",
      "ResourceConfig": {
        "folderPath": "Shared Documents/General/${uprn}"
      },
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "$authenticated",
      "allows": [
        "*"
      ]
    }
  ]
}
